--source include/have_router.inc

--let $extra_http_enable_ssl=0
--let $extra_logger_info=1
--source ../include/predefined_setup/configure_router_mrs_root.inc
--source ../include/schema/sakila-schema.sql
--source ../include/schema/sakila-data.sql


# The test uses HOST to have only paths in HREF fields.
--let $mrs_add_service_path="/svc"
--let $mrs_add_host_name=""
--source ../include/mrs/add_service.inc

--let $mrs_add_schema=sakila
--let $mrs_add_schema_path=/sakila
--source ../include/mrs/db_schema/add.inc

--let $mrs_add_db_object=city
--let $mrs_add_db_object_path=/city
--source ../include/mrs/db_object/add.inc

--let $mrs_add_db_object=actor
--let $mrs_add_db_object_path=/actor
--source ../include/mrs/db_object/add.inc

--let $mrs_add_db_object=address
--let $mrs_add_db_object_path=/address
--source ../include/mrs/db_object/add.inc

--let $mrs_add_db_object=category
--let $mrs_add_db_object_path=/category
--source ../include/mrs/db_object/add.inc

--let $mrs_add_db_object=country
--let $mrs_add_db_object_path=/country
--source ../include/mrs/db_object/add.inc

--let $mrs_add_db_object=film
--let $mrs_add_db_object_path=/film
--source ../include/mrs/db_object/add.inc

--let $mrs_add_db_object=payment
--let $mrs_add_db_object_path=/payment
--source ../include/mrs/db_object/add.inc


--exec $MRS_CLIENT_ARGS --path /svc/sakila/city/1 --wait-until-found 60

echo # The test enviroment is read.;
echo # $MRS_URL !;
--exec_in_background /home/lukasz/test_sakila.sh $MRS_URL >/home/lukasz/test_sakila.out
sleep 43;

cat_file /home/lukasz/test_sakila.out;

# Cleanup
DROP SCHEMA sakila;
--source ../include/mrs/cleanup.inc
